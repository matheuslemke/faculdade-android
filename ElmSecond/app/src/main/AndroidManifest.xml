<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="br.com.lemke.tcc.elmsecond">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_brain_tree"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".TrainActivity"
            android:label="Train">
        </activity>
        <activity
            android:name=".ResultsActivity"
            android:label="Results">
        </activity>
        <activity
            android:name=".TestFileActivity"
            android:label="Test File">
        </activity>
        <activity
            android:name=".TestUniqueActivity"
            android:label="Test Input">
        </activity>
        <activity android:name=".FileDialogActivity">
        </activity>
        <activity android:name=".ListElmsActivity">
        </activity>
        <activity android:name=".TestDebugActivity">
        </activity>
    </application>

</manifest>